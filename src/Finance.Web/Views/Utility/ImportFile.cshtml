@using System

@{
    ViewBag.Title = "File Import";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.ContentHeader = "Import File";
}
<div id="v-fileimport">
    <div class="level">
        <div class="level-left">
            <div class="level-item">
                <div class="title">Import File</div>
            </div>
        </div>
    </div>
    <div class="columns is-multiline">
        <div class="column is-12">
            <form method="post" enctype="multipart/form-data" action="/utility/importfile">
                @Html.AntiForgeryToken()
                <div class="field">
                    <div class="file is-info">
                        <label class="file-label">
                            <input type="file" name="csvfile" />
                        </label>
                    </div>
                </div>
                <div class="field">
                    <button type="submit" class="button is-primary">Import</button>
                </div>
            </form>
        </div>
    </div>
</div>

@section extrajs
{
    
}

@section extrascripts
{
    
}